{
    "name": "healdash",
    "version": "1.0.0",
    "private": true,
    "main": "main.js",
    "scripts": {
        "start": "vue-cli-service build && electron .",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "package-mac": "vue-cli-service build && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=res/mac/icon.icns --prune=true --out=release-builds",
        "package-win": "vue-cli-service build && electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=res/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Healdash\""
    },
    "dependencies": {
        "axios": "^0.21.1",
        "buefy": "^0.9.4",
        "bulma": "^0.9.2",
        "chart.js": "^2.9.4",
        "core-js": "^3.9.0",
        "dayjs": "^1.10.4",
        "electron": "^12.0.1",
        "jwt-decode": "^3.1.2",
        "lodash": "^4.17.20",
        "numeral": "^2.0.6",
        "register-service-worker": "^1.7.2",
        "vue": "^2.6.12",
        "vue-axios": "^3.2.4",
        "vue-chartjs": "^3.5.1",
        "vue-router": "^3.5.1",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.5.11",
        "@vue/cli-plugin-eslint": "^4.5.11",
        "@vue/cli-plugin-pwa": "^4.5.11",
        "@vue/cli-plugin-router": "^4.5.11",
        "@vue/cli-plugin-vuex": "^4.5.11",
        "@vue/cli-service": "^4.5.11",
        "@vue/eslint-config-standard": "^5.1.2",
        "babel-eslint": "^10.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-vue": "^7.6.0",
        "gh-pages": "^3.1.0",
        "sass": "^1.32.7",
        "sass-loader": "^10.1.1",
        "vue-template-compiler": "^2.6.12"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
